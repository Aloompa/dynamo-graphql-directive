{
  "name": "@aloompa/dynamo-graphql",
  "version": "1.0.0",
  "description": "Dynamo Directive for GraphQL",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --no-coverage --maxWorkers=2",
    "test:watch": "jest --watch --no-coverage",
    "build": "./node_modules/.bin/tsc",
    "start": "./node_modules/.bin/tsc --watch"
  },
  "author": "Aloompa",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.535.0",
    "graphql": "^14.5.7",
    "graphql-tools": "^4.0.5",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "apollo-server-testing": "^2.9.7",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "apollo-server": "^2.9.7"
  },
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "\\.(ts|tsx)$": "./node_modules/ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx)$",
    "collectCoverage": true,
    "collectCoverageFrom": ["src/**/*.{ts,tsx}"],
    "coverageReporters": ["html", "json"]
  }
}
